{
  "name": "@sanity/tsdoc-to-portable-text",
  "version": "0.1.14",
  "sideEffects": false,
  "source": "src/index.ts",
  "exports": {
    "cli": "./lib/tsdoc-to-portable-text.cli.js",
    "require": "./lib/tsdoc-to-portable-text.js",
    "default": "./lib/tsdoc-to-portable-text.modern.js"
  },
  "main": "lib/tsdoc-to-portable-text.js",
  "module": "lib/tsdoc-to-portable-text.module.js",
  "types": "lib/src/index.d.ts",
  "bin": {
    "tsdoc-to-portable-text": "lib/cli.js"
  },
  "files": [
    "lib",
    "src"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "scripts": {
    "build": "run-s build:microbundle copy:json && chmod +x lib/cli.js",
    "build:microbundle": "microbundle --format modern,esm,cjs --target node --tsconfig tsconfig.lib.json src/{cli,index}.ts",
    "clean": "rimraf lib",
    "copy:json": "cpx 'src/extract/*.json' lib/",
    "lint": "eslint . --ext .js,.jsx,.mjs,.ts,.tsx --quiet",
    "prebuild": "yarn clean",
    "test": "jest --runInBand",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@microsoft/api-extractor": "^7.18.19",
    "@microsoft/api-extractor-model": "^7.13.16",
    "@microsoft/tsdoc": "^0.13.2",
    "@types/cpx": "^1.5.2",
    "chalk": "^4.1.2",
    "mkdirp": "^1.0.4",
    "pkg-up": "^3.1.0",
    "slugify": "^1.6.3",
    "tmp": "^0.2.1",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@types/mkdirp": "^1.0.2",
    "@types/tmp": "^0.2.2",
    "cpx": "^1.5.0"
  }
}

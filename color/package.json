{
  "name": "@sanity/color",
  "version": "2.0.7",
  "description": "The Sanity color palette.",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "sideEffects": false,
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "run-p build-*",
    "build-cjs": "tsc --baseUrl . --module CommonJS --outDir dist/cjs --project tsconfig.dist.json --rootDir src --target ES5",
    "build-es": "tsc --baseUrl . --module ES6 --outDir dist/es --project tsconfig.dist.json --rootDir src --target ES6",
    "clean": "rimraf dist",
    "generate": "ts-node scripts/build.ts",
    "lint": "eslint . --ext .js,.ts",
    "prebuild": "npm run clean && npm run generate",
    "prepublishOnly": "npm run build",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^14.14.6",
    "@types/prettier": "^2.0.2",
    "cssnano": "^4.1.10",
    "polished": "^4.0.3",
    "prettier": "^2.1.1",
    "ts-node": "^9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/design.git"
  },
  "keywords": [
    "sanity",
    "color",
    "palette"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/design/issues"
  },
  "homepage": "https://www.sanity.io/",
  "publishConfig": {
    "access": "public"
  }
}

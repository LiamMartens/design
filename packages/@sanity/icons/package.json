{
  "name": "@sanity/icons",
  "version": "1.2.4",
  "description": "The Sanity icons.",
  "sideEffects": false,
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "require": "./lib/sanity-icons.js",
      "default": "./lib/sanity-icons.modern.js"
    }
  },
  "main": "./lib/sanity-icons.js",
  "module": "./lib/sanity-icons.module.js",
  "types": "./lib/dts/index.d.ts",
  "files": [
    "./lib/",
    "./src/"
  ],
  "license": "MIT",
  "scripts": {
    "build": "../../../bin/pkg-utils bundle --tsconfig tsconfig.lib.json --validate",
    "clean": "rimraf lib",
    "generate": "node -r esbuild-register scripts/generate.ts",
    "lint": "eslint . --ext .js,.jsx,.mjs,.ts,.tsx --quiet",
    "prebuild": "yarn clean",
    "pregenerate": "rimraf src/icons",
    "test": "jest",
    "type-check": "tsc -b",
    "watch": "../../../bin/pkg-utils bundle --tsconfig tsconfig.lib.json --watch"
  },
  "devDependencies": {
    "@svgr/core": "^5.5.0",
    "@types/mkdirp": "^1.0.2",
    "camelcase": "^6.3.0",
    "glob": "^7.2.0",
    "react": "^17.0.2"
  },
  "peerDependencies": {
    "react": "^16.9 || ^17"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/design.git"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/design/issues"
  },
  "keywords": [
    "sanity",
    "ui",
    "icons"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "homepage": "https://www.sanity.io/",
  "publishConfig": {
    "access": "public"
  }
}

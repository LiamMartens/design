{
  "name": "@sanity/ui-workshop",
  "version": "0.3.4",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "build": "run-s build:esbuild build:dts copy:dts:* clean:dts",
    "build:esbuild": "node -r esbuild-register scripts/build.ts",
    "build:dts": "tsc --emitDeclarationOnly --project tsconfig.lib.json --rootDir src",
    "clean": "rimraf lib",
    "clean:dts": "rimraf lib/dts",
    "copy:dts:cjs": "cpx 'lib/dts/**/*.{d.ts,d.ts.map}' lib/cjs",
    "copy:dts:esm": "cpx 'lib/dts/**/*.{d.ts,d.ts.map}' lib/esm",
    "lint": "eslint . --ext .js,.jsx,.mjs,.ts,.tsx --quiet",
    "prebuild": "yarn clean",
    "type-check": "tsc -b"
  },
  "dependencies": {
    "axe-core": "^4.3.3",
    "pako": "^2.0.4",
    "segmented-property": "^3.0.3"
  },
  "devDependencies": {
    "@sanity/icons": "^1.2.1",
    "@sanity/ui": "^0.36.12",
    "@types/pako": "^1.0.2",
    "react": "^17.0.2",
    "styled-components": "^5.3.1"
  },
  "peerDependencies": {
    "@sanity/icons": "^1.0",
    "@sanity/ui": "^0.33",
    "react": "^17",
    "styled-components": "^5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/design.git"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/design/issues"
  },
  "keywords": [
    "sanity",
    "ui",
    "components"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "homepage": "https://www.sanity.io/",
  "publishConfig": {
    "access": "public"
  }
}

{
  "private": true,
  "name": "sanity-design-docs",
  "version": "0.1.61",
  "scripts": {
    "build": "next build",
    "clean": "rimraf .next workshopScopes.ts",
    "dev": "run-s scopes:build && run-p scopes:watch next:dev",
    "lint": "eslint . --ext .js,.jsx,.mjs,.ts,.tsx --quiet",
    "next:dev": "next dev",
    "prebuild": "run-s clean scopes:build",
    "scopes:build": "node -r esbuild-register scripts/scopes/build.ts",
    "scopes:watch": "node -r esbuild-register scripts/scopes/watch.ts",
    "start": "next start",
    "test": "jest",
    "tsc:no-emit": "tsc --noEmit",
    "type-check": "run-s scopes:build tsc:no-emit"
  },
  "dependencies": {
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/color": "^2.1.4",
    "@sanity/icons": "^1.1.5",
    "@sanity/logos": "^1.1.4",
    "@sanity/ui": "^0.36.1",
    "@sanity/ui-workshop": "^0.1.25",
    "codemirror": "^5.62.2",
    "date-fns": "^2.23.0",
    "lodash": "^4.17.21",
    "next": "^11.1.0",
    "next-sanity": "^0.4.0",
    "pako": "^2.0.4",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^17.0.2",
    "slugify": "^1.6.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@types/codemirror": "^5.60.2",
    "@types/pako": "^1.0.2",
    "@types/qs": "^6.9.7",
    "babel-plugin-styled-components": "^1.13.2",
    "chokidar": "^3.5.2"
  }
}

{
  "private": true,
  "name": "sanity-ui-storybook",
  "version": "0.1.7",
  "main": "package.json",
  "scripts": {
    "build": "build-storybook --quiet --config-dir . --output-dir public",
    "clean": "rimraf public",
    "dev": "start-storybook --ci --quiet --config-dir . --port 9009",
    "lint": "eslint . --ext .js,.jsx,.mjs,.ts,.tsx",
    "prebuild": "npm run clean",
    "predev": "rimraf node_modules/.cache",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@sanity/icons": "^1.0.1",
    "@sanity/ui": "^0.31.4",
    "@storybook/addon-a11y": "^6.1.10",
    "@storybook/addon-actions": "^6.1.10",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.1.10",
    "@storybook/addon-links": "^6.1.10",
    "@storybook/react": "^6.1.10",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@types/refractor": "^3.0.0",
    "@types/styled-components": "^5.1.4"
  }
}
